{
  "name": "@nymphjs/driver-mysql",
  "version": "1.0.0-beta.33",
  "description": "NymphJS - MySQL DB Driver",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "nymph",
    "ORM",
    "object relational mapper"
  ],
  "scripts": {
    "clean": "test -d dist && rm -r dist || true",
    "build": "tsc; cp src/runMysqlSync.js dist/runMysqlSync.js",
    "watch": "tsc --watch",
    "prepare": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:db:start": "docker run --rm --name nymph-mysql -e MYSQL_ROOT_PASSWORD=123nymphroot -e MYSQL_DATABASE=nymph -e MYSQL_USER=nymph -e MYSQL_PASSWORD=nymph -p 3306:3306 mysql",
    "test:db:stop": "docker kill -s TERM nymph-mysql"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sciactive/nymphjs.git"
  },
  "author": "Hunter Perrin <hperrin@gmail.com>",
  "bugs": {
    "url": "https://github.com/sciactive/nymphjs/issues"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@nymphjs/guid": "^1.0.0-beta.33",
    "@nymphjs/nymph": "^1.0.0-beta.33",
    "mysql2": "^3.2.4",
    "sqlstring": "^2.3.3"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/jest": "^29.5.1",
    "@types/sqlstring": "^2.3.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
