{
  "name": "@nymphjs/driver-mysql",
  "version": "1.0.0-alpha.35",
  "description": "NymphJS - MySQL DB Driver",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "nymph",
    "ORM",
    "object relational mapper"
  ],
  "scripts": {
    "clean": "test -d dist && rm -r dist || true",
    "build": "tsc;  cp src/runMysqlSync.js dist/runMysqlSync.js",
    "watch": "tsc --watch",
    "prepare": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:db:run": "npm run test:db:start && npm run test:db:clean",
    "test:db:clean": "docker container rm nymph-mysql",
    "test:db:start": "docker run --name nymph-mysql -e MYSQL_ROOT_PASSWORD=123nymphroot -e MYSQL_DATABASE=nymph -e MYSQL_USER=nymph -e MYSQL_PASSWORD=nymph -p 3306:3306 mysql",
    "test:db:stop": "docker kill -s TERM nymph-mysql"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sciactive/nymphjs.git"
  },
  "author": "Hunter Perrin <hperrin@gmail.com>",
  "bugs": {
    "url": "https://github.com/sciactive/nymphjs/issues"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@nymphjs/nymph": "^1.0.0-alpha.35",
    "@vlasky/mysql": "^2.18.5",
    "nanoid": "^3.1.30"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^27.4.0",
    "@types/mysql": "^2.15.20",
    "jest": "^27.4.7",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  }
}
