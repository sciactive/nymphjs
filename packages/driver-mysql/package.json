{
  "name": "@nymphjs/driver-mysql",
  "version": "1.0.0-alpha.0",
  "description": "NymphJS - MySQL DB Driver",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "nymph",
    "ORM",
    "object relational mapper"
  ],
  "scripts": {
    "clean": "test -d dist && rm -r dist || true",
    "build": "tsc;  cp src/runMysqlSync.js dist/runMysqlSync.js",
    "watch": "tsc --watch",
    "prepare": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:mysql:run": "npm run test:mysql:start && npm run test:mysql:clean",
    "test:mysql:clean": "docker container rm nymph",
    "test:mysql:start": "docker run --name nymph -e MYSQL_ROOT_PASSWORD=123nymphroot -e MYSQL_DATABASE=nymph -e MYSQL_USER=nymph -e MYSQL_PASSWORD=nymph -p 3306:3306 mysql",
    "test:mysql:stop": "docker kill -s TERM nymph"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sciactive/nymphjs.git"
  },
  "author": "Hunter Perrin <hperrin@gmail.com>",
  "bugs": {
    "url": "https://github.com/sciactive/nymphjs/issues"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@nymphjs/nymph": "^1.0.0-alpha.0",
    "@vlasky/mysql": "^2.18.5"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^27.0.1",
    "@types/mysql": "^2.15.19",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5"
  }
}
