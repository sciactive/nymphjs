{
  "name": "@nymphjs/tilmeld-setup",
  "version": "1.0.0-beta.83",
  "description": "Nymph.js - Tilmeld Setup App",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "nymph",
    "ORM",
    "object relational mapper"
  ],
  "scripts": {
    "clean": "test -d dist && rm -r dist || true",
    "build": "npm run build:server && npm run build:app && npm run build:app:theme",
    "build:server": "tsc -p tsconfig.server.json",
    "build:app": "NODE_ENV=\"production\" rollup -c",
    "build:app:theme": "smui-theme compile dist/app/smui.css -i app/theme",
    "watch:server": "tsc --watch -p tsconfig.server.json",
    "watch:app": "NODE_ENV=\"development\" rollup -c --watch",
    "prepublish": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sciactive/nymphjs.git"
  },
  "author": "Hunter Perrin <hperrin@gmail.com>",
  "bugs": {
    "url": "https://github.com/sciactive/nymphjs/issues"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@nymphjs/nymph": "^1.0.0-beta.83",
    "@nymphjs/tilmeld": "^1.0.0-beta.83",
    "express": "^4.21.2",
    "locutus": "^2.0.32"
  },
  "devDependencies": {
