    "@sciactive/splitn": "^3.1.0",
    "email-templates": "^12.0.1",
    "express": "^4.21.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "locutus": "^2.0.32",
    "lodash-es": "^4.17.21",
