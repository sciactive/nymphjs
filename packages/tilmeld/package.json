    "@types/jsonwebtoken": "^9.0.7",
    "@types/locutus": "^0.0.8",
    "@types/lodash-es": "^4.17.12",
    "@types/qrcode": "^1.5.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
