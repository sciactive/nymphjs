    "nanoid": "^5.1.6",
    "nanoid-dictionary": "^5.0.0",
    "sha1": "^1.1.1"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.13",
    "@types/jest": "^30.0.0",
    "@types/luxon": "^3.7.1",
    "@types/nanoid-dictionary": "^4.2.3",
    "@types/sha1": "^1.1.5",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
