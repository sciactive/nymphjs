    "nanoid": "^5.0.9",
    "nanoid-dictionary": "^4.3.0",
    "sha1": "^1.1.1"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.8",
    "@types/jest": "^29.5.14",
    "@types/luxon": "^3.4.2",
    "@types/nanoid-dictionary": "^4.2.3",
    "@types/sha1": "^1.1.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
