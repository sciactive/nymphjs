  "devDependencies": {
    "@nymphjs/client": "^1.0.0-beta.1",
    "@nymphjs/client-node": "^1.0.0-beta.1",
    "@nymphjs/driver-sqlite3": "^1.0.0-beta.1",
    "@tsconfig/recommended": "^1.0.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
