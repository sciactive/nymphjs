    "@nymphjs/client-node": "^1.0.0-beta.80",
    "@nymphjs/driver-sqlite3": "^1.0.0-beta.80",
    "@tsconfig/recommended": "^1.0.3",
    "@types/cookie-parser": "^1.4.7",
