{
  "name": "@nymphjs/tilmeld-components",
  "version": "1.0.0-alpha.18",
  "description": "NymphJS - Tilmeld Front End Components",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "src/index.d.ts",
  "svelte": "lib/index.js",
  "keywords": [
    "nymph",
    "ORM",
    "object relational mapper"
  ],
  "scripts": {
    "clean": "npm run clean:ts && npm run clean:js",
    "clean:ts": "test -d lib && rm -r lib || true",
    "clean:js": "test -d dist && rm -r dist || true",
    "build": "npm run build:ts && npm run build:svelte && npm run build:bundle",
    "build:bundle": "webpack",
    "watch:bundle": "webpack --watch",
    "build:ts": "tsc",
    "watch:ts": "tsc --watch",
    "build:svelte": "node ./preprocess.js",
    "prepare": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sciactive/nymphjs.git"
  },
  "author": "Hunter Perrin <hperrin@gmail.com>",
  "bugs": {
    "url": "https://github.com/sciactive/nymphjs/issues"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@nymphjs/tilmeld-client": "^1.0.0-alpha.18",
    "@smui/button": "^6.0.0-beta.2",
    "@smui/circular-progress": "^6.0.0-beta.2",
    "@smui/dialog": "^6.0.0-beta.2",
    "@smui/form-field": "^6.0.0-beta.2",
    "@smui/radio": "^6.0.0-beta.2",
    "@smui/textfield": "^6.0.0-beta.2",
    "@tsconfig/recommended": "^1.0.1",
    "@tsconfig/svelte": "^2.0.1",
    "@types/jest": "^27.0.2",
    "jest": "^27.3.1",
    "svelte": "^3.44.1",
    "svelte-check": "^2.2.10",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.9.8",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "~4.4.4",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1"
  }
}
