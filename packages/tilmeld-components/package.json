{
  "name": "@nymphjs/tilmeld-components",
  "version": "1.0.0-beta.6",
  "description": "NymphJS - Tilmeld Front End Components",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "lib/index.js",
  "svelte": "lib/index.js",
  "types": "src/index.d.ts",
  "keywords": [
    "nymph",
    "ORM",
    "object relational mapper"
  ],
  "scripts": {
    "clean": "npm run clean:ts && npm run clean:js",
    "clean:ts": "test -d lib && rm -r lib || true",
    "clean:js": "test -d dist && rm -r dist || true",
    "build": "npm run build:ts && npm run build:svelte && npm run build:bundle",
    "build:bundle": "webpack",
    "watch:bundle": "webpack --watch",
    "build:ts": "tsc",
    "watch:ts": "tsc --watch",
    "build:svelte": "svelte-strip strip src/ lib/",
    "prepare": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sciactive/nymphjs.git"
  },
  "author": "Hunter Perrin <hperrin@gmail.com>",
  "bugs": {
    "url": "https://github.com/sciactive/nymphjs/issues"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@nymphjs/tilmeld-client": "^1.0.0-beta.6",
    "@smui/button": "^7.0.0-beta.0",
    "@smui/circular-progress": "^7.0.0-beta.0",
    "@smui/dialog": "^7.0.0-beta.0",
    "@smui/form-field": "^7.0.0-beta.0",
    "@smui/radio": "^7.0.0-beta.0",
    "@smui/textfield": "^7.0.0-beta.0",
    "@tsconfig/recommended": "^1.0.1",
    "@tsconfig/svelte": "^3.0.0",
    "@types/jest": "^29.2.5",
    "jest": "^29.3.1",
    "svelte": "^3.55.0",
    "svelte-check": "^3.0.1",
    "svelte-loader": "^3.1.4",
    "svelte-preprocess": "^5.0.0",
    "svelte-strip": "^1.0.3",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "typescript": "~4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
