{
  "name": "@nymphjs/nymph",
  "version": "1.0.0-beta.99",
  "description": "Nymph.js - Nymph ORM",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "nymph",
    "ORM",
    "object relational mapper"
  ],
  "scripts": {
    "clean": "test -d dist && rm -r dist || true",
    "build": "tsc",
    "watch": "tsc --watch",
    "prepublish": "npm run clean && npm run build",
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" npx jest",
    "test:watch": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" npx jest --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sciactive/nymphjs.git"
  },
  "author": "Hunter Perrin <hperrin@gmail.com>",
  "bugs": {
    "url": "https://github.com/sciactive/nymphjs/issues"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@nymphjs/guid": "^1.0.0-beta.99",
    "@sciactive/tokenizer": "^3.0.2",
    "debug": "^4.4.0",
    "joi": "^17.13.3",
    "locutus": "^2.0.32",
    "lodash-es": "^4.17.21",
    "n-readlines": "^1.0.3"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.8",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.14",
    "@types/locutus": "^0.0.8",
    "@types/lodash-es": "^4.17.12",
    "@types/n-readlines": "^1.0.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
