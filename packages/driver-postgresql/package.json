{
  "name": "@nymphjs/driver-postgresql",
  "version": "1.0.0-beta.107",
  "description": "Nymph.js - PostgreSQL DB Driver",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "nymph",
    "ORM",
    "object relational mapper"
  ],
  "scripts": {
    "clean": "test -d dist && rm -r dist || true",
    "build": "tsc",
    "watch": "tsc --watch",
    "prepublish": "npm run clean && npm run build",
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" npx jest",
    "test:watch": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" npx jest --watch",
    "test:db:start": "docker run --rm --name nymph-postgresql -e POSTGRES_USER=nymph -e POSTGRES_PASSWORD=nymph -p 5432:5432 postgres",
    "test:db:stop": "docker kill -s TERM nymph-postgresql"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sciactive/nymphjs.git"
  },
  "author": "Hunter Perrin <hperrin@gmail.com>",
  "bugs": {
    "url": "https://github.com/sciactive/nymphjs/issues"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@nymphjs/guid": "^1.0.0-beta.107",
    "@nymphjs/nymph": "^1.0.0-beta.107",
    "@sciactive/tokenizer": "^3.0.2",
    "pg": "^8.18.0",
    "pg-cursor": "^2.17.0",
    "pg-format": "^1.0.4"
  },
  "devDependencies": {
    "@nymphjs/tilmeld": "^1.0.0-beta.107",
    "@tsconfig/recommended": "^1.0.13",
    "@types/jest": "^30.0.0",
    "@types/pg": "^8.16.0",
    "@types/pg-cursor": "^2.7.2",
    "@types/pg-format": "^1.0.5",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
